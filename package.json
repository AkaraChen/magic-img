{
  "name": "magic-img",
  "version": "0.0.2",
  "description": "让你的图片加载更加优雅",
  "scripts": {
    "go": "pnpm -r --filter=./packages/core --no-git-checks publish",
    "eslint": "eslint .",
    "build": "tsup",
    "build:all": "tsup && pnpm build:example",
    "build:example": "pnpm -r --filter=./example build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./css": "./dist/index.css"
  },
  "keywords": [],
  "author": "lishaobos",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "jimp": "^0.16.2",
    "lqip": "^2.1.0",
    "potrace": "^2.1.8",
    "sqip": "^0.3.3",
    "tsup": "^6.5.0",
    "webp-converter": "^2.3.3"
  }
}
